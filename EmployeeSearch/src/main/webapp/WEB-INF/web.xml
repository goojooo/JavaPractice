<?xml version="1.0" encoding="UTF-8"?>
<web-app>
<welcome-file-list>
    <welcome-file>search.html</welcome-file>
</welcome-file-list>


    <servlet>
        <servlet-name>SearchEmployee</servlet-name>
        <servlet-class>pages.SearchEmployeeServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SearchEmployee</servlet-name>
        <url-pattern>/searchEmployee</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>EmployeeResult</servlet-name>
        <servlet-class>pages.EmployeeResultServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>EmployeeResult</servlet-name>
        <url-pattern>/employeeResult</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Error</servlet-name>
        <servlet-class>pages.ErrorServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Error</servlet-name>
        <url-pattern>/error</url-pattern>
    </servlet-mapping>

</web-app>
